<!DOCTYPE html>
<html lang="en">
  <head>
    <title>I'm Hades</title>

    <!-- meta -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- stylesheet -->
    <link rel="stylesheet" href="css/bootstrap-5.0.2-dist/bootstrap.min.css" />
    <!-- <link rel="stylesheet" href="css/mdb/mdb.min.css" /> -->
    <!-- <link rel="stylesheet" href="css/mdb/file-upload.min.css" /> -->
    <link rel="stylesheet" href="css/bootstrap-tagsinput.css" />
    <link rel="stylesheet" href="css/font-awesome.min.css" />
    <link rel="stylesheet" href="css/jsoneditor.min.css" />
    <link rel="stylesheet" href="css/animate.css" />
    <link rel="stylesheet" href="css/toast.css" />
    <!-- <link rel="stylesheet" href="css/mdb/core.min.css" /> -->

    <link rel="shortcut icon" href="./img/favicon.ico" type="image/x-icon" />

    <link rel="stylesheet" href="stylesheets/scrollTop.css" />
    <link rel="stylesheet" href="stylesheets/admin.css" />

    <script src="js/vue.min.js"></script>
    <script src="js/jquery-3.7.0.min.js"></script>

    <script src="js/jsoneditor.min.js"></script>

    <style>
      .label {
        display: inline;
        padding: 0.2em 0.6em 0.3em;
        font-size: 75%;
        font-weight: 700;
        line-height: 1;
        color: #fff;
        text-align: center;
        white-space: nowrap;
        vertical-align: baseline;
        border-radius: 0.25em;
      }
      .label-info {
        background-color: #5bc0de;
      }
      .bootstrap-tagsinput {
        width: 100%;
      }
      #site-header {
        position: fixed;
        width: 100%;
        /* background-color: #35405f; */
        background-color: rgba(144, 146, 153, 0.94);
        /* background-color: rgba(32, 178, 170, 0.86); */
        padding: 0 30px;
        z-index: 100;
      }

      #site-header h1 {
        margin: 0;
        font-family: "Oswald", sans-serif;
        margin-top: 11px;
        font-size: 26px;
        font-weight: 300;
        letter-spacing: 2px;
      }

      #site-header h1 a:focus {
        text-decoration: none;
      }

      #site-header h1 a {
        color: #fff;
      }

      #site-header h1 a:hover {
        text-decoration: none;
      }
    </style>
  </head>
  <body>
    <div class="content-block fixed-top" id="header">
      <header id="site-header" class="clearfix navbar navbar-expand-lg">
        <div class="container-fluid m-0 p-0">
          <h1 class="navbar-brand mt-1 mb-1">
            <a href="/login" class="text-decoration-none">I'm Hades</a>
          </h1>
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <i class="fa fa-bars fa-2x text-light"></i>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <div class="ms-auto d-flex me-2">
              <div>
                <a href="/" class="text-light text-decoration-none ms-4 me-3"
                  ><i class="fa fa-home"></i> <span>Home</span></a
                >
              </div>
              <div>
                <a
                  href="/portfolio"
                  class="text-light text-decoration-none ms-4 me-3"
                  ><i class="fa fa-bookmark"></i> Portfolio</a
                >
              </div>
              <div>
                <a
                  href="/aboutme"
                  class="text-light text-decoration-none ms-4 me-3"
                  ><i class="fa fa-bullhorn"></i> Me</a
                >
              </div>
              <div>
                <a
                  href="#contact"
                  class="text-light text-decoration-none ms-4 me-3"
                  ><i class="fa fa-phone-square"></i> Contact</a
                >
              </div>
            </div>
            <!-- /.navbar-collapse -->
          </div>
        </div>
      </header>
    </div>

    <%- include("repos") %>

    <br/>
    
    <%- include("database") %>

    <div class="container border"></div>

    <%- include("aboutme") %>

    <div class="container border"></div>

    <%- include("contacts") %>

    <div class="container border"></div>

    <footer id="site-footer" class="fixed-bottom bg-secondary text-white">
      <div class="container">
        <div class="row">
          <div class="col-sm-6">
            <div class="copyright">&copy; 2019 HADES</div>
          </div>
          <div class="col-sm-6">
            <div class="designed-by">
              Developed with
              <a href="#" target="_blank" class="text-white fw-bold"
                >EVN(Expressjs, Vue.js, Node.js)</a
              >
            </div>
          </div>
        </div>
      </div>
      <div id="scroll-Top">
        <div class="return-to-top">
          <i class="fa fa-angle-up" id="scroll-top"></i>
        </div>
      </div>
    </footer>

    <script src="js/bootstrap-5.0.2-dist/bootstrap.min.js"></script>
    <!-- <script src="js/mdb/mdb.min.js"></script> -->
    <!-- <script src="js/mdb/core.min.js"></script> -->
    <!-- <script src="js/mdb/search.min.js"></script> -->
    <!-- <script src="js/mdb/main.min.js"></script> -->
    <script src="js/mdb/file-upload.min.js"></script>
    <script src="js/mdb/bootstrap-tagsinput.min.js"></script>
    <script src="js/jquery.toast.min.js"></script>

    <script src="js/admin.js"></script>
    <script src="javascripts/scrollTop.js"></script>
  </body>
</html>
